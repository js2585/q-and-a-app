<%-include("partials/header2")%>

<div class="container">
    <div class="login">
        <h1 style="text-align: center">Sign Up</h1>
        <form action="/register" method="POST">
            <div class="form-group">
                <label for="Username">Username </label>
                <div>
                    <input class="form" type="text" name="username" id="Username" maxlength="15">
                </div>
            </div>
            <div class="form-group">
                <label for="Password">Password </label>
                <div>
                    <input class="form" type="text" name="password" id="Password">
                </div>
            </div>
            <div class="form-group">
                <input class="btn btn-primary btn-md form" type="submit" value="Sign Up">
            </div>
        </form>
    </div>
</div>


<%-include("partials/footer2")%>
